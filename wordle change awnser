javascript:(function(e,result){if(/nytimes.com\/games\/wordle/.test(location.href)){result=prompt('New word');if(result&&result.length==5){e=JSON.parse(window.localStorage.getItem('nyt-wordle-state'));e.boardState=['','','','','',''];e.evaluations=[null,null,null,null,null,null];e.rowIndex=0;e.gameStatus='IN_PROGRESS';e.solution=result.toLowerCase();e.lastPlayedTs=Date.now();window.localStorage.setItem('nyt-wordle-state',JSON.stringify(e));window.location.reload()}}})()

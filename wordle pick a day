javascript:(function(e,result){if(/nytimes.com\/games\/wordle/.test(location.href)){var day=document.querySelector('game-app').dayOffset;day=prompt('Pick a day (0-'+day+')');result=new XMLHttpRequest();result.open('GET','https://dracos.co.uk/made/wordle-bookmarklet/words.php?day=%27+day,false);result.send(null);result=result.response;e=JSON.parse(window.localStorage.getItem(%27nyt-wordle-state%27));e.boardState=[%27%27,%27%27,%27%27,%27%27,%27%27,%27%27];e.evaluations=[null,null,null,null,null,null];e.rowIndex=0;e.gameStatus=%27IN_PROGRESS%27;e.solution=result.toLowerCase();e.lastPlayedTs=Date.now();window.localStorage.setItem(%27nyt-wordle-state%27,JSON.stringify(e));window.location.reload()}})()

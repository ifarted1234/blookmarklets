javascript:(function(e,result){if(/nytimes.com\/games\/wordle/.test(location.href)){var day=document.querySelector('game-app').dayOffset;result=new XMLHttpRequest();result.open('GET','https://dracos.co.uk/made/wordle-bookmarklet/words.php?day=%27+(day-1),false);result.onload=function(){result=result.response;if(result&&result.length==5){e=JSON.parse(window.localStorage.getItem(%27nyt-wordle-state%27));e.boardState=[%27%27,%27%27,%27%27,%27%27,%27%27,%27%27];e.evaluations=[null,null,null,null,null,null];e.rowIndex=0;e.gameStatus=%27IN_PROGRESS%27;e.solution=result.toLowerCase();e.lastPlayedTs=Date.now();window.localStorage.setItem(%27nyt-wordle-state%27,JSON.stringify(e));window.location.reload()}};result.send(null);}})()
